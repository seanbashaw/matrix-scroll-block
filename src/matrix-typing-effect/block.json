{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/matrix-typing-effect",
	"version": "0.1.0",
	"title": "Matrix Typing Effect",
	"category": "widgets",
	"description": "Creates a cool scrolling effect for text.",
	"example": {},
	"attributes": {
		"matrixText": {
			"type": "array",
			"default": []
		},
		"matrixID": {
			"type": "string"
		},
		"matrixSpeed": {
			"type": "number",
			"default": 10
		},
		"startDelay": {
			"type": "number",
			"default": 1.0
		},
		"betweenDelay": {
			"type":"number",
			"default": 1.0	
		},
		"onView": {
			"type": "booelan",
			"default": false
		}

	},
	"supports": {
		"anchor": true,
		"align": true,
		"position" true,
		"interactivity": true,
		"color": {
			"background": false,
			"text" :true
		},
		"html": false,
		"typography":{
			"fontSize": true
		}
	},
	"textdomain": "matrix-typing-effect",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
